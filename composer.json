{
    "name": "ricorocks-digital-agency/going-postal",
    "description": "A Laravel package that provides a driver based postal code lookup integration.",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "livewire/livewire": "^2.4",
        "pestphp/pest-plugin-expectations": "^1.0",
        "spatie/laravel-ray": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "RicorocksDigitalAgency\\GoingPostal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RicorocksDigitalAgency\\GoingPostal\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "luke",
            "email": "lukeraymonddowning@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "RicorocksDigitalAgency\\GoingPostal\\Providers\\GoingPostalServiceProvider"
            ],
            "aliases": {
                "GoingPostal": "RicorocksDigitalAgency\\GoingPostal\\Facades\\GoingPostal"
            }
        }
    },
    "minimum-stability": "stable"
}
